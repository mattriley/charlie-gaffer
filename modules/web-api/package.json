{
  "name": "web-api",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "4.6.1"
  },
  "scripts": {
    "deploy-app": "npm run npm-install && npm run heroku-deploy",
    "deploy-infrastructure": "`syscfg` && AWS_PROFILE=${AWS_DEPLOYMENT_PROFILE} lakitu",
    "npm-install": "linklocal && linklocal --unlink && npm install",
    "start": "node server.js",
    "start-local": "`syscfg` && heroku local",
    "heroku-deploy": "`syscfg` && heroku config:push --overwrite -a ${SYSTEM_NAMESPACE_30} && heroku builds:create -a ${SYSTEM_NAMESPACE_30}",
    "heroku-logs": "`syscfg` && heroku logs -a ${SYSTEM_NAMESPACE_30}"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "aws-sdk": "^2.6.9",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "node-fetch": "^1.6.3",
    "node-uuid": "^1.4.7"
  },
  "devDependencies": {
    "awscred": "^1.2.0",
    "lakitu": "0.0.6",
    "system": "file:../system"
  }
}
